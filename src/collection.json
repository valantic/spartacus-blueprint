{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "feature": {
      "description": "Creates a feature blueprint.",
      "factory": "./blueprints/index#feature",
      "schema": "./blueprints/schema.json"
    },
    "cms-component": {
      "description": "Creates a cms component blueprint.",
      "factory": "./blueprints/index#cmsComponent",
      "schema": "./blueprints/schema.json"
    },
    "simple-component": {
      "description": "Creates a simple component blueprint.",
      "factory": "./blueprints/index#simpleComponent",
      "schema": "./blueprints/schema.json"
    }
  }
}
